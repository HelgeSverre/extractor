{
    "preset": "laravel",
    "rules": {
        "php_unit_fqcn_annotation": true,
        "fully_qualified_strict_types": true,
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "global_namespace_import": {
            "import_classes": true,
            "import_constants": null,
            "import_functions": null
        }
    }
}
